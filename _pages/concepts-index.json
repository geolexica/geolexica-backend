---
permalink: "/api/concepts.json"
---
{
  {% for concept in site.concepts %}
  {% assign json_concept = concept.representations.json %}
  "{{ concept.termid }}": {
    "term": "{{ concept.term }}",
    "termid": {{ concept.termid }},
    "uri-html": "{{ concept.url }}",
    "uri-json": "{{ json_concept.url }}"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}
